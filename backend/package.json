{
    "name": "online-compiler-backend",
    "version": "1.0.0",
    "description": "Backend server for online compiler with Docker execution",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "tsx watch --ignore code --ignore output server.ts",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "compiler",
        "docker",
        "code-execution"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.14.12",
        "nodemon": "^3.0.1",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4"
    }
}
