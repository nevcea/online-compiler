{
    "name": "online-compiler-backend",
    "version": "1.0.0",
    "description": "Backend server for online compiler with Docker execution",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "tsx watch --ignore code --ignore output server.ts",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [
        "compiler",
        "docker",
        "code-execution"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.5",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.10.1",
        "@types/supertest": "^6.0.3",
        "jest": "^30.2.0",
        "nodemon": "^3.0.1",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4"
    }
}
