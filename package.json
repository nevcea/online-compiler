{
    "name": "online-compiler",
    "version": "1.0.0",
    "description": "Online code compiler with multiple language support",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.{js,json,css,html}\"",
        "format:check": "prettier --check \"**/*.{js,json,css,html}\"",
        "dev": "node scripts/dev.cjs",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && npm run dev",
        "test": "node scripts/test.cjs",
        "test:backend": "cd backend && npm test",
        "test:backend:watch": "cd backend && npm run test:watch",
        "test:backend:coverage": "cd backend && npm run test:coverage",
        "clean": "node scripts/clean.cjs",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "eslint": "^9.15.0",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "prettier": "^3.3.3"
    }
}
